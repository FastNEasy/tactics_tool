        "eslint-plugin-vue": "^7.9.0",
        "eslint-webpack-plugin": "^2.5.4",
        "jquery": "^3.2",
        "laravel-mix": "^4.0.7",
        "laravel-mix-eslint": "^0.2.0",
        "lint-staged": "^11.0.0",
        "lodash": "^4.17.13",
